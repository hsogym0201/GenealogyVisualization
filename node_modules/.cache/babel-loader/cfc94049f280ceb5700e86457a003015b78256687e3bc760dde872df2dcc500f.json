 ${params.data.target}<br>值: ${params.data.value}`;\r\n          },\r\n        },\r\n        series: [\r\n          {\r\n            type: \"sankey\",\r\n            layout: \"none\", // 禁用自动布局\r\n            data: sankeyData.nodes,\r\n            links: sankeyData.links,\r\n            emphasis: {\r\n              focus: \"adjacency\",\r\n            },\r\n            layoutIterations: 64, // 增加布局优化次数\r\n            nodeAlign: \"justify\", // 节点左对齐\r\n            levels: [\r\n              { depth: 0, itemStyle: { color: \"#e1d5e7\" } }, // 第一列\r\n              { depth: 1, itemStyle: { color: \"#d5e8d4\" } }, // 第二列\r\n              { depth: 2, itemStyle: { color: \"#dae8fc\" } }, // 第三列\r\n              { depth: 3, itemStyle: { color: \"#ffe6cc\" } }, // 第四列\r\n            ],\r\n            lineStyle: {\r\n              color: \"source\",\r\n              opacity: 0.6,\r\n            },\r\n          },\r\n        ],\r\n        graphic: [\r\n          // 添加色块和标题\r\n          {\r\n            type: \"rect\",\r\n            left: \"5%\",\r\n            top: \"1%\",\r\n            shape: {\r\n              width: 20,\r\n              height: 20,\r\n            },\r\n            style: {\r\n              fill: \"#e1d5e7\", // 第一列的颜色\r\n            },\r\n          },\r\n          {\r\n            type: \"text\",\r\n            left: \"6%\",\r\n            top: \"1%\",\r\n            style: {\r\n              text: \"始祖原居地\",\r\n              fontSize: 16,\r\n              fontWeight: \"bold\",\r\n            },\r\n          },\r\n          {\r\n            type: \"rect\",\r\n            left: \"29.5%\",\r\n            top: \"1%\",\r\n            shape: {\r\n              width: 20,\r\n              height: 20,\r\n            },\r\n            style: {\r\n              fill: \"#d5e8d4\", // 第二列的颜色\r\n            },\r\n          },\r\n          {\r\n            type: \"text\",\r\n            left: \"30%\",\r\n            top: \"1%\",\r\n            style: {\r\n              text: \"始祖迁居地\",\r\n              fontSize: 16,\r\n              fontWeight: \"bold\",\r\n            },\r\n          },\r\n          {\r\n            type: \"rect\",\r\n            left: \"54%\",\r\n            top: \"1%\",\r\n            shape: {\r\n              width: 20,\r\n              height: 20,\r\n            },\r\n            style: {\r\n              fill: \"#dae8fc\", // 第三列的颜色\r\n            },\r\n          },\r\n          {\r\n            type: \"text\",\r\n            left: \"55%\",\r\n            top: \"1%\",\r\n            style: {\r\n              text: \"第三列标题\",\r\n              fontSize: 16,\r\n              fontWeight: \"bold\",\r\n            },\r\n          },\r\n          {\r\n            type: \"rect\",\r\n            left: \"78%\",\r\n            top: \"1%\",\r\n            shape: {\r\n              width: 20,\r\n              height: 20,\r\n            },\r\n            style: {\r\n              fill: \"#ffe6cc\", // 第四列的颜色\r\n            },\r\n          },\r\n          {\r\n            type: \"text\",\r\n            left: \"78%\",\r\n            top: \"1%\",\r\n            style: {\r\n              text: \"第四列标题\",\r\n              fontSize: 16,\r\n              fontWeight: \"bold\",\r\n            },\r\n          },\r\n        ],\r\n      };\r\n\r\n      this.myChart.setOption(option);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.SangJiTu {\r\n  width: 100%;\r\n  text-align: center;\r\n}\r\n#SangJiTU {\r\n  width: 100%;\r\n  height: 700px;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAAU;;uBAArBC,mBAAA,CAEM,OAFNC,UAEM,EAAAC,MAAA,QAAAA,MAAA,OADJC,mBAAA,CAAyB;IAApBC,EAAE,EAAC;EAAU,2B","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}